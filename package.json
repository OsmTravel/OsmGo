{
  "name": "osm_go",
  "author": "Fabien Del Olmo",
  "description": "Map them all !",
  "homepage": "https://github.com/DoFabien/OsmGo",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration production",
    "build:ghpages": "ng build --configuration production-ghpages",
    "test": "ng test --ts-config src/tsconfig.spec.json --browsers Chrome",
    "test:coverage": "npm run test -- --code-coverage",
    "test:ci": "ng test --ts-config src/tsconfig.spec.json --watch=false --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sprites": "node ./scripts/importMissingSVGs.js && node ./scripts/formatSVG.js && node  ./scripts/sprites.js",
    "importFromID": "node ./scripts/importTagsFromID.js && node ./scripts/ordersTags.js && node ./scripts/importDeprecatdeTagsFromiD.js && node ./scripts/addTranslationFromiD.js ",
    "importBaseMaps": "node ./scripts/importBasemaps.js",
    "buildAndroid": "node ./incrementVersion.js && ionic build --prod --release && npx cap sync && npx cap open android",
    "buildAndroidDebug": "ng build --configuration development && npx cap sync",
    "prepare": "husky install",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write ."
  },
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/forms": "~12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@angular/service-worker": "~12.2.8",
    "@capacitor/android": "^3.2.4",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.2.4",
    "@capacitor/device": "^1.0.3",
    "@capacitor/geolocation": "^1.1.0",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/keyboard": "^1.2.2",
    "@capacitor/splash-screen": "^1.1.3",
    "@capacitor/status-bar": "^1.0.8",
    "@ionic/angular": "^5.5.2",
    "@ionic/storage": "^3.0.6",
    "@ionic/storage-angular": "^3.0.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/turf": "^5.1.6",
    "angular-resize-event": "^3.0.2",
    "fast-xml-parser": "^4.0.8",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.20",
    "maplibre-gl": "^2.1.9",
    "martinez-polygon-clipping": "^0.7.3",
    "moment": "^2.29.1",
    "ngx-moment": "^5.0.0",
    "osm-auth": "github:DoFabien/osm-auth#master",
    "rxjs": "~6.6.0",
    "save-svg-as-png": "^1.4.17",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular-eslint/builder": "~12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/compiler-cli": "~12.2.8",
    "@angular/language-service": "~12.0.1",
    "@capacitor/cli": "^3.2.4",
    "@ionic/angular-toolkit": "^4.0.0",
    "@types/geojson": "^7946.0.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.162",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "cheerio": "^1.0.0-rc.3",
    "codelyzer": "^6.0.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "fs-extra": "^9.0.1",
    "husky": "^8.0.1",
    "jasmine": "^3.6.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-stringify-pretty-compact": "^2.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^13.0.2",
    "node-fetch": "^2.6.7",
    "parse-svg-path": "^0.1.2",
    "prettier": "2.7.1",
    "protractor": "~7.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "scale-svg-path": "0.0.1",
    "serialize-svg-path": "^0.1.0",
    "source-map-explorer": "^2.5.0",
    "spritesmith": "^3.4.0",
    "svg-render": "^1.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4",
    "xml2js": "^0.4.23",
    "yargs": "^14.2.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
